# Add your environment variables here

AUTH_SECRET="replace-with-a-long-random-string"
ADMIN_EMAIL="admin@example.com"
ADMIN_PASSWORD="password"

# Database (Postgres)
# Example: postgres://USER:PASSWORD@HOST:PORT/DB_NAME
# For Neon/Supabase, paste the full connection string provided by the service.
DATABASE_URL=

#--Firecrawl
FIRECRAWL_API_KEY=


#--EXA
EXA_API_KEY=

BETTER_AUTH_URL=

BEDROCK_MODEL_ID=

EXA_API_KEY=

# Redis (Amazon ElastiCache)
# Set these in Amplify environment variables after enabling VPC access.
# Use cluster config endpoint with REDIS_MODE=cluster, or primary endpoint with REDIS_MODE=single.
REDIS_MODE=single
REDIS_HOST=
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD=
REDIS_TLS=true
REDIS_KEY_PREFIX=cm:

